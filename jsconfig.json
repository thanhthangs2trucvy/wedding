{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES6",
    "baseUrl": "./src",
    "paths": {
      "@assets/*": ["./src/assets/*"],
      "@images/*": ["./src/assets/images/*"],
      "@common": ["./src/common/index"],
      "@config": ["./src/config/index"],
      "@components": ["./src/components/index"],
      "@components/*": ["./src/components/*"],
      "@helpers/*": ["./src/helpers/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@redux/*": ["./src/redux/*"],
      "@actions/*": ["./src/redux/actions/*"],
      "@reducers/*": ["./src/redux/reducers/*"],
      "@routes/*": ["./src/routes/*"],
      "@utils/*": ["./src/utils/*"],
      "@service/*": ["./src/services/*"],
      "*": ["declare/*", "*"]
    }
  },
  "include": ["src/**/*"]
}
